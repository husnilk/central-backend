meta {
  name: Add User
  type: http
  seq: 2
}

post {
  url: http://{{host}}:{{port}}/api/users/
  body: formUrlEncoded
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:form-urlencoded {
  email: newuser@example.com
  password: password
  firstName: New
  lastName: User
  Role: USER
  supervisorId: {{user_id}}
}
